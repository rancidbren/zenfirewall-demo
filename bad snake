def process_user_data(a, b, c, d, e, f, g):
    result = 0
    if a > 0:
        if b > 1:
            if c == 3:
                for i in range(10):
                    if d < i:
                        result += 1
                    else:
                        result -= 1
            else:
                result += c
        else:
            result -= b
    else:
        if e and f:
            result += g
        else:
            result = -1
    return result
